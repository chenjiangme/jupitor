# Jupitor Environment Configuration
# Copy to .env and fill in values

# Storage volumes
DATA_1=/mnt/data1
# DATA_2=/mnt/data2  # Future overflow volume

# Alpaca API (US market data + trading)
ALPACA_API_KEY=
ALPACA_API_SECRET=
ALPACA_BASE_URL=https://paper-api.alpaca.markets  # paper trading
# ALPACA_BASE_URL=https://api.alpaca.markets      # live trading

# Server
JUPITOR_HOST=0.0.0.0
JUPITOR_PORT=8080
JUPITOR_GRPC_PORT=9090

# Logging
JUPITOR_LOG_LEVEL=info
